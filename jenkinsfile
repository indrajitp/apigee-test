pipeline {
    agent any

    stages {
		stage('CheckOutSourceCode') {
            steps {
                git 'https://github.com/indrajitp/apigee-test.git'
            }
        }
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
